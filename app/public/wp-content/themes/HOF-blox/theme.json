{	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [],
	"settings": {
		"appearanceTools": true,
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": false,
			"defaultGradients": true,
			"defaultPalette": true,
			"gradients": [],
			"palette": [
				{
					"color": "#55951b",
					"name": "avocado",
					"slug": "custom-avocado"
				},
				{
					"color": "#78be21",
					"name": "kelly",
					"slug": "custom-kelly"
				},
				{
					"color": "#97d700",
					"name": "leaf",
					"slug": "custom-leaf"
				},
				{
					"color": "#ccc",
					"name": "ccc",
					"slug": "custom-grey"
				},
				{
					"color": "#095256",
					"name": "midnight",
					"slug": "custom-midnight"
				},
				{
					"color": "#e9ec68",
					"name": "pear",
					"slug": "custom-pear"
				}
			],
			"text": true
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"customSpacingSize": true,
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			],
			"spacingScale": {
				"operator": "*",
				"increment": 1.5,
				"steps": 7,
				"mediumStep": 1.5,
				"unit": "rem"
			},
			"spacingSizes": []
		},
		"layout": {
		  "contentSize": "1440px",
		  "wideSize": "72vw"
		},
		"typography": {
			"customFontSize": true,
			"dropCap": true,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Apotek Comp",
							"fontStretch": "normal",
							"fontStyle": "normal",
						  	"fontWeight": "",
							"src": [
								"https://use.typekit.net/qyi0ffp.css"
							]
						}
					],
					"fontFamily": "Apotek Comp, sans-serif",
					"name": "Apotek Comp",
					"slug": "apotek-comp"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Proxima Nova",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"https://use.typekit.net/qyi0ffp.css"
							]
						}
					],
					"fontFamily": "Proxima Nova, sans-serif",
					"name": "Proxima Nova",
					"slug": "proxima-nova"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Proxima Nova",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "300",
							"src": [
								"https://use.typekit.net/qyi0ffp.css"
							]
						}
					],
					"fontFamily": "Proxima Nova Italic, sans-serif",
					"name": "Proxima Nova Italic",
					"slug": "proxima-nova-it"
				},
			  {
				"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
				"name": "System Font",
				"slug": "system-font"
			  }
			],
			"fontSizes": [
				{
					"size": "22px",
					"slug": "small"
				},
				{
					"size": "28px",
					"slug": "medium"
				},
				{
					"size": "36px",
					"slug": "large"
				},
				{
					"size": "48px",
					"slug": "x-large"
				},
				{
					"size": "72px",
					"slug": "xx-large"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textColumns": true,
			"textTransform": true
		}

	},

	"styles": {
		"blocks": {
			"core/columns": {
				"spacing": {
					"margin": {},
					"blockGap": {}
				}
			},
			"core/gallery": {
				"spacing": {
					"blockGap": "1rem",
					"margin": {}
				}
			},
			"core/group": {
				"css": "display: flex;\nflex-flow: column;\njustify-content: space-between;\nalign-items: center;\nmax-width: 72vw;\nmargin: 0 auto;"
			},
			"core/image": {
				"css": "display: flex;\nflex-flow: column;\njustify-content: flex-start;\nalign-items: stretch;\nmargin: 0;"
			},
			"core/navigation": {
				"css": "display: flex;\njustify-content: space-between;\nalign-items: center;",
				"elements": {
					"link": {
						":active": {
							"color": {
								"text": "var(--wp--preset--color--custom-leaf)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--custom-leaf)"
							}
						},
						":hover": {
							"typography": {
								"textDecoration": ".5px underline dotted"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"blockGap": "1.5rem"
				}
			},
			"core/navigation-link": {
				"css": "",
				"typography": {
					"fontFamily": "apotek-comp, sans-serif",
					"fontSize": "22px",
					"fontStyle": "normal",
					"fontWeight": "300",
					"letterSpacing": "2px",
					"lineHeight": "1.0",
					"textTransform": "uppercase"
				}
			},
			"core/navigation-submenu": {
				"css": "",
				"typography": {
					"fontFamily": "apotek-comp, sans-serif",
					"fontSize": "22px",
					"fontStyle": "normal",
					"fontWeight": "300",
					"letterSpacing": "2px",
					"lineHeight": "1.0",
					"textTransform": "uppercase"
				}
			},
			"core/post-title": {
				"css": "display: flex;\nflex-flow: column;\njustify-content: flex-start;\ntext-shadow: 1px 1px 1px #0006;\nwidth: 100%",
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--custom-midnight)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--custom-leaf)"
						}
					}
				},
				"spacing": {
					"padding": {
						"left": "0"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--apotek-comp)",
					"lineHeight": "1",
					"textTransform": "uppercase"
				}
			}
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--custom-kelly)"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--custom-leaf)"
				}
			},
			"h1": {
				"color": {
					"text": "var(--wp--preset--color--custom-leaf)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--apotek-comp)",
					"fontSize": "clamp(3rem, calc(3rem + ((1vw - 0.48rem) * 8.4135)), 6rem)",
					"fontWeight": "300",
					"letterSpacing": "1px",
					"lineHeight": ".9",
				  	"textTransform": "uppercase"
				}
			},
			"h2": {
				"color": {
					"text": "var(--wp--preset--color--custom-leaf)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--apotek-comp)",
					"fontSize": "clamp(2.625rem, calc(2.625rem + ((1vw - 0.48rem) * 8.4135)), 4rem)",
					"fontWeight": "300",
					"letterSpacing": "3px",
					"lineHeight": ".9",
				  	"textTransform": "uppercase"
				}
			},
			"h3": {
				"color": {
					"text": "var(--wp--preset--color--custom-midnight)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--apotek-comp)",
					"fontSize": "clamp(2rem, calc(2rem + ((1vw - 0.48rem) * 8.4135)), 3.25rem)",
					"fontWeight": "300",
					"letterSpacing": "2px",
					"lineHeight": "1",
				  	"textTransform": "uppercase"
				}
			},
			"h4": {
				"color": {
					"text": "var(--wp--preset--color--custom-midnight)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--apotek-comp)",
					"fontSize": "2.5rem",
					"fontWeight": "500",
					"letterSpacing": "1px",
					"lineHeight": ".9",
					"textTransform": "uppercase"
				}
			},
			"h5": {
				"color": {
					"text": "var(--wp--preset--color--custom-midnight)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--proxima-nova)",
					"fontSize": "2rem",
					"fontWeight": "800",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"color": {
					"text": "var(--wp--preset--color--custom-midnight)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--proxima-nova)",
					"fontSize": "24px",
					"fontWeight": "800",
					"letterSpacing": "3px",
					"lineHeight": "1.3",
				  	"textTransform": "uppercase"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--custom-leaf)"
					}
				},
				"typography": {
					"fontSize": "true",
					"textDecoration": "true"
				}
			}
		}
	},
	"templateParts": []
}